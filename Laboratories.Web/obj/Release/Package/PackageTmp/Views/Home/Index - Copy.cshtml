@using Laboratories.Domain;


<div class="row tile_count">
    <div class="col-md-3">
        <a href="/Companies/Index" title="@Laboratories.Web.Resources.General.Company">
            @{
                if ((User)Session["UserInfo"] != null)
                {
                    User oUser = (User)Session["UserInfo"];
                    if (ViewBag.companyimage != null)
                    {
                        @Html.Raw("<img width=\"250px\"  height=\"250px\" class=\"rounded-circle\" src=\"data:image/jpeg;base64," + @Convert.ToBase64String(ViewBag.companyimage) + "\"  />");

                    }
                    else
                    {
                        <img src="~/Design/build/images/Company.png" alt=" @Laboratories.Web.Resources.General.Company" class="rounded-circle">
                    }
                }
            }


        </a>
