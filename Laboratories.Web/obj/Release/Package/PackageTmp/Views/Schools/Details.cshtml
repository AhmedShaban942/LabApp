        <strong style="text-align:center;color:darkblue">@Laboratories.Web.Resources.General.Schools </strong>
    </h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Laboratories.Web.Resources.General.Complexes
        </dt>

        <dd>
            @Html.DisplayFor(model => model.sch_complex.comp_arName)
        </dd>

        <dt>
