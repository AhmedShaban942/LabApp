        </dt>

        <dd>
            @Html.DisplayFor(model => model.sch_enName)
        </dd>

        <dt>
            @Laboratories.Web.Resources.General.phon
        </dt>

        <dd>
            @Html.DisplayFor(model => model.sch_phon)
        </dd>

        <dt>
            @Laboratories.Web.Resources.General.email
        </dt>

        <dd>
            @Html.DisplayFor(model => model.sch_email)
        </dd>

        <dt>
            @Laboratories.Web.Resources.General.address
        </dt>

        <dd>
            @Html.DisplayFor(model => model.sch_address)
        </dd>
        <dt>
            @Laboratories.Web.Resources.General.laboratoryRecordName
        </dt>

        <dd>
            @Html.DisplayFor(model => model.sch_laboratoryRecordName)
        </dd>
       

    </dl>
</div>
<p>

    @Html.ActionLink(@Laboratories.Web.Resources.General.Edit, "Edit", new { id = Model.Id }, new { @class = "btn btn-info" })

    @Html.ActionLink(@Laboratories.Web.Resources.General.backToList, "Index")
</p>
