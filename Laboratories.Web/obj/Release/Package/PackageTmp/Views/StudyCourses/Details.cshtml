@model Laboratories.Domain.StudyCourse

@{
    ViewBag.Title = Laboratories.Web.Resources.StudentRates.studyCourse;
}



<div>
    <h4 align="center" class="panel-title ">
        <strong style="text-align:center;color:darkblue">@Laboratories.Web.Resources.StudentRates.studyCourse </strong>
    </h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Laboratories.Web.Resources.General.arName
        </dt>

        <dd>
            @Html.DisplayFor(model => model.cors_arName)
        </dd>

        <dt>
            @Laboratories.Web.Resources.General.enName
        </dt>

        <dd>
            @Html.DisplayFor(model => model.cors_enName)
        </dd>

        <dt>
            @Laboratories.Web.Resources.Item.itm_department
        </dt>

        <dd>
            @Html.DisplayFor(model => model.cors_department)
        </dd>

        <dt>
            @Laboratories.Web.Resources.Item.itm_level
        </dt>

        <dd>
            @Html.DisplayFor(model => model.cors_level)
        </dd>

        <dt>
            @Laboratories.Web.Resources.Item.itm_term
        </dt>

        <dd>
            @Html.DisplayFor(model => model.cors_term)
        </dd>

        <dt>
            @Laboratories.Web.Resources.StudentRates.degree
        </dt>

        <dd>
            @Html.DisplayFor(model => model.cors_degree)
        </dd>

        <dt>
            @Laboratories.Web.Resources.StudentRates.rate
        </dt>

        <dd>
            @Html.DisplayFor(model => model.cors_rate)
        </dd>

    </dl>
</div>
<p>

    @Html.ActionLink(@Laboratories.Web.Resources.General.Edit, "Edit", new { id = Model.Id }, new { @class = "btn btn-info" })

    @Html.ActionLink(@Laboratories.Web.Resources.General.backToList, "Index")
</p>


